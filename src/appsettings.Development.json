{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "JointLengthSequencing": "Debug"
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "*"
    ]
  },
  "Authentication": {
    "ApiKeys": [
      "dev-api-key-12345",
      "test-api-key-67890"
    ]
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 300
      },
      {
        "Endpoint": "*",
        "Period": "1h",
        "Limit": 10000
      }
    ]
  }
}
